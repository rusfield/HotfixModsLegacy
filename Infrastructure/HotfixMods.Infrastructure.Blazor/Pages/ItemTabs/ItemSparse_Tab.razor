@code {
    ItemSparseInfo info = new();

    async Task<Dictionary<long, string>> GetAllowableRaces()
    {
        var resultList = Enum.GetValues<ItemSparseAllowableRace>().ToDictionary(key => (long)key, value => value.ToDisplayString()).ToList();

        resultList.Sort((firstPair, nextPair) =>
        {
            if (firstPair.Key == (long)ItemSparseAllowableRace.ALL || firstPair.Key == (long)ItemSparseAllowableRace.ANY_ALLIANCE_RACE || firstPair.Key == (long)ItemSparseAllowableRace.ANY_HORDE_RACE)
            {
                return -1;
            }
            else if (nextPair.Key == (long)ItemSparseAllowableRace.ALL || nextPair.Key == (long)ItemSparseAllowableRace.ANY_ALLIANCE_RACE || nextPair.Key == (long)ItemSparseAllowableRace.ANY_HORDE_RACE)
            {
                return 1;
            }
            else
            {
                return firstPair.Key.CompareTo(nextPair.Key);
            }
        });

        return resultList.ToDictionary(pair => pair.Key, pair => pair.Value);
    }

    async Task<Dictionary<sbyte, string>> GetInventoryTypes()
    {
        return Enum.GetValues<ItemInventoryType>().ToDictionary(key => (sbyte)key, value => value.ToDisplayString());
    }

    async Task<Dictionary<sbyte, string>> GetOverallQualityIds()
    {
        return Enum.GetValues<ItemSparseOverallQualityId>().ToDictionary(key => (sbyte)key, value => value.ToDisplayString());
    }

    async Task<Dictionary<byte, string>> GetBondings()
    {
        return Enum.GetValues<ItemSparseBonding>().ToDictionary(key => (byte)key, value => value.ToDisplayString());
    }
}