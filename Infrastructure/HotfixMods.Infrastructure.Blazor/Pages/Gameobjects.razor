@using HotfixMods.Infrastructure.Blazor.Components.Dialogs;
@using HotfixMods.Infrastructure.Blazor.Components.DtoContent

@inject GameobjectService Service
@inherits PageBase<GameobjectDto>

<DtoTabsContent T="GameobjectDto" SaveAsync_Callback="SaveAsync" ValueCompare_Callback="@(async () => ValueCompareAsync<GameobjectSearch_Dialog>())" />


@code {

    // Temp
    protected override void OnParametersSet()
    {
        if (null == PageTab.Dto)
            PageTab.Dto = new GameobjectDto();
        base.OnParametersSet();
    }

    async void SaveAsync()
    {
        if (PageTab.Dto != null)
            await Service.SaveAsync((GameobjectDto)PageTab.Dto);
    }
}
