<MudContainer>
    <MudPaper Height="600px" Class="mt-10 pa-2">
        <MudText Typo="Typo.h5" Style="color:var(--mud-palette-action-disabled); width:100%" Align="Align.Center">
            Latest Update
        </MudText>
        <MudPaper Style="height:1px; width:100%;background-color:var(--mud-palette-action-disabled)" />
    </MudPaper>
    <MudPaper Height="144px" Class="mt-5 pa-2">
        <MudText Typo="Typo.h5" Style="color:var(--mud-palette-action-disabled); width:100%" Align="Align.Center">
            Credits
        </MudText>
        <MudPaper Style="height:1px; width:100%;background-color:var(--mud-palette-action-disabled)" />
        <MudGrid>
            <MudItem xs="4">
                <CreditBox Title="TrinityCore Developers" Description="... and Community for server side documentation and maintaining a world to play in." IsGitHubAccount="false" />
            </MudItem>
            <MudItem xs="4">
                <CreditBox Title="WoWDev" Description="... for supplying tools and definitions to read and load DB2 files." IsGitHubAccount="true" />
            </MudItem>
            <MudItem xs="4">
                <CreditBox Title="Marlamin" Description="... and contributors for resources regarding DB2 contents." IsGitHubAccount="true" />
            </MudItem>
        </MudGrid>
    </MudPaper>
</MudContainer>

