<MudCarouselItem Transition="Transition.None">
    @* The remaining px-4 is in PagedDtoContent *@
    <MudGrid Class="px-2 mt-7">
        @ChildContent
    </MudGrid>
</MudCarouselItem>

@code {
    [CascadingParameter(Name = "Transition")]
    public Transition Transition { get; set; }

    [Parameter]
    public RenderFragment ChildContent { get; set; }
}
